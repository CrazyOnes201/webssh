<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.liu.entity.Traininfo" table="traininfo" schema="tots">
        <id name="id" column="id" >
            <generator class="uuid" />
        </id>
        <property name="trainId" column="trainId" />
        <property name="station" column="station" />
        <property name="startTime" column="startTime"/>
        <property name="endTime" column="endTime"/>
        <property name="rank" column="rank"/>
        <property name="miles" column="miles"/>
    </class>
</hibernate-mapping>