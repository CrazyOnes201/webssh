<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.liu.entity.Trainmile" table="trainmile" schema="tots">
        <composite-id mapped="true" class="com.liu.entity.TrainmilePK">
            <key-property name="trainId" column="trainId"/>
            <key-property name="station" column="station"/>
        </composite-id>
        <property name="miles" column="miles"/>
    </class>
</hibernate-mapping>