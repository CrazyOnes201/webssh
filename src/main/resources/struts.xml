<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <package name="user" extends="struts-default">
        <action name="regist" class="registAction">
            <result name="success">/successregister.jsp</result>
            <result name="input">/index.jsp</result>
        </action>

        <action name="login" class="loginAction">
            <!--<result name="success">/mainPage.jsp</result>-->
            <result name="success">/SearchTicket.jsp</result>
            <result name="input">/register.jsp</result>
        </action>

        <action name="searchticket" class="searchTicketAction">
            <result name="success">/SearchTicket.jsp</result>
            <result name="login">/register.jsp</result>
        </action>

    </package>
</struts>